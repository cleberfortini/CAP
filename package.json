{
  "name": "cap_finance",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/postgres": "^2.1.2",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-app-categories": "cds watch --open br.com.cap.category.appcategories/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-app-wallets": "cds watch --open br.com.cap.carteiras.appwallets/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-app-movements-types": "cds watch --open br.com.cap.movements.types.appmovementstypes/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-app-movements-type": "cds watch --open br.com.cap.movements.types.appmovementstype/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-app-movements": "cds watch --open br.com.cap.movements.appmovements/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/app-categories",
    "app/app-wallets",
    "app/app-movements-types",
    "app/app-movements-type",
    "app/app-movements"
  ]
}
